<script>
$(document).ready(function(){
	$(function() {
		$( "#sortable" ).sortable({
			revert: true
		});
		$( "#draggable" ).draggable({
			connectToSortable: "#sortable",
			helper: "clone",
			revert: "invalid"
		});
		$( "ul", "li" ).disableSelection();
	});

    $("#selecctall").change(function(){
      $(".filled-in").prop('.checked', $(this).prop(".checked"));
    });

    

});


</script>

